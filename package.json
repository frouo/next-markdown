{
  "name": "next-markdown",
  "version": "0.0.4",
  "description": "Static pages generated from markdown files for your Next.js website.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rm -rf dist/ && tsc",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouo/next-markdown.git"
  },
  "keywords": [
    "nextjs",
    "markdown",
    "git",
    "generator",
    "static",
    "pages"
  ],
  "author": "Fran√ßois Rouault <francois.rouault@cocoricostudio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frouo/next-markdown/issues"
  },
  "homepage": "https://github.com/frouo/next-markdown#readme",
  "devDependencies": {
    "@types/jest": "27.4.1",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "gray-matter": "4.0.3",
    "rehype-remark": "9.1.2",
    "rehype-stringify": "9.0.3",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "unified": "10.1.1"
  }
}
